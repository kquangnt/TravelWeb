<h1>Create a new Province</h1>

<%= form_for(@province, url: admin_provinces_path) do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name, class: 'form-control' %>

  <%= f.label :description %>
  <%= f.text_area :description, class: 'form-control' %>

  <%= f.submit "Save Change", class: "btn btn-primary" %>
<% end %>