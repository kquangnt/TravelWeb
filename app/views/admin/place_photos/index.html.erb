<h1>Place Photos Management</h1>

<div class="row">
  <%= collection_select(:place, :province_id, Province.all, :id, :name, 
      {onchange: :x = "getPlaces()"} ) %>
  <%= collection_select(:place_photo, :place_id, 
      Place.getPlacesOfProvince(:x), :id, :name) %>
</div>

<script>
  function getPlaces() {
    var x = document.getElementById("place_province_id");
    return x;
  }
</script>