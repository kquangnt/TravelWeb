<h1>User: <%= @user.name %> </h1>

<div class="container">
<%= link_to "Edit", edit_admin_user_path, method: :get %>
<%= link_to "Delete", admin_user_path, method: :delete,
    data: {confirm: "Are you sure?"} %>
</div>

<div>
  <h3>Email: <%= @user.email %></h3>
  <h5>Admin? <%= check_box_tag(:is_admin, 1, @user.is_admin?, disabled: true) %></h5>
  <h5>Created_at: <%= @user.created_at %></h5>
</div>

<%= link_to "Back", admin_users_path %>